{
  "name": "nest-angular-ionic-starter",
  "version": "1.0.0",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "setup-client": "cd client-web && npm install ",
    "setup-server": "cd server && npm install",
    "heroku-prebuild": "concurrently \"npm run setup-client\" \"npm run setup-server\"",
    "build": "cd client-web && npm run build:prod && cd ../server && npm run build && mv ../client-web/dist/* ../server/dist/client-web",
    "start": "cd .. && npm run start:prod"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  }
}
