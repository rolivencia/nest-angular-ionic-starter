{
  "name": "nest-angular-ionic-starter",
  "version": "1.0.0",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "build": "node build-stack.js",
    "heroku-prebuild": "npm run setup-client && npm run setup-server",
    "lint": "cd server && npm run lint && cd ../client-web && npm run lint",
    "postinstall": "husky install",
    "prettify": "npx prettier --write .",
    "setup-client": "cd client-web && npm install",
    "setup-server": "cd server && npm install",
    "start": "cd server && npm run start:prod"
  },
  "dependencies": {
    "copyfiles": "^2.4.1"
  },
  "devDependencies": {
    "husky": "^5.0.9",
    "lint-staged": "^10.5.4",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0"
  }
}
